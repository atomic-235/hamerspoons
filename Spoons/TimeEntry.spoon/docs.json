{
  "TimeEntry": {
    "description": "Spoon for entering current date and time in ISO format",
    "author": "Atomic235",
    "version": "1.0",
    "license": "MIT - https://opensource.org/licenses/MIT",
    "homepage": "https://github.com/Hammerspoon/Spoons",
    "methods": {
      "bindHotkeys": {
        "signature": "TimeEntry:bindHotkeys(mapping)",
        "description": "Binds hotkeys for TimeEntry",
        "parameters": {
          "mapping": "A table containing hotkey modifier/key key pairs. Defaults to TimeEntry.defaultHotkey"
        },
        "returns": "TimeEntry object"
      },
      "init": {
        "signature": "TimeEntry:init()",
        "description": "Initialize TimeEntry spoon",
        "parameters": {},
        "returns": "TimeEntry object"
      },
      "insertCurrentTime": {
        "signature": "TimeEntry:insertCurrentTime()",
        "description": "Gets current date and time and inserts it at cursor position or copies to clipboard",
        "parameters": {},
        "returns": "None"
      },
      "start": {
        "signature": "TimeEntry:start()",
        "description": "Start the TimeEntry spoon",
        "parameters": {},
        "returns": "TimeEntry object"
      },
      "stop": {
        "signature": "TimeEntry:stop()",
        "description": "Stop the TimeEntry spoon",
        "parameters": {},
        "returns": "TimeEntry object"
      }
    },
    "variables": {
      "defaultHotkey": {
        "description": "Default hotkey to insert current time",
        "default": "{\"cmd\", \"ctrl\"}, \"t\""
      },
      "logger": {
        "description": "Logger object used within the Spoon. Can be accessed to set the default log level for the messages coming from the Spoon."
      }
    },
    "downloads": ["https://github.com/Hammerspoon/Spoons"],
    "source": [
      "https://github.com/Hammerspoon/Spoons/blob/master/Spoons/TimeEntry.spoon/init.lua"
    ]
  }
}
